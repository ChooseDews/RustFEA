NAME BAR Simulation
DOF 3

#MESH
MESH_FILE ./meshes/tube.inp #example comment

SOLVE_METHOD explicit
TIME_STEP 0.00001
TIME_STEPS 100000
#PRINT_STEPS 10000000
VTK_SAVE_STEPS 500
OUTPUT_VTK ./temp/tube/tube_explicit.vtk


#Boundary Conditions
load_bc end1 10000 10000 0
